{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "icons": [
        "icons"
      ],
      "components": [
        "components"
      ],
      "models": [
        "models"
      ],
      "interfaces": [
        "interfaces"
      ],
      "types": [
        "types"
      ],
      "data": [
        "data"
      ],
      "config": [
        "config"
      ]
    }
  },

  "include": [
    "src"
  ]
}
